{
  "policies": [
    {
      "handler": {
        "export": "ApiKeyInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "allowUnauthenticatedRequests": false,
          "cacheTtlSeconds": 60
        }
      },
      "name": "api-key-inbound",
      "policyType": "api-key-inbound"
    },
    {
      "handler": {
        "export": "MonetizationInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "allowRequestsWithoutSubscription": false,
          "allowRequestsOverQuota": false,
          "meterOnStatusCodes": "200-299",
          "meters": {
            "requests": 1,
            "computeUnits": 3
          }
        }
      },
      "name": "monetization-inbound",
      "policyType": "monetization-inbound"
    },
    {
      "handler": {
        "export": "CompositeInboundPolicy",
        "module": "$import(@zuplo/runtime)",
        "options": {
          "policies": [
            "api-key-inbound",
            "monetization-inbound"
          ]
        }
      },
      "name": "monetization",
      "policyType": "composite-inbound"
    }
  ]
}
